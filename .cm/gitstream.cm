# -*- mode: yaml -*-

manifest:
  version: 1.0
  
automations:
  dispatch_action:
    if:
      - {{ files | match(term='src/ios/') | some }}
    run:
      - action: run-github-workflow@v1
        args:
          workflow: .github/workflows/ios.yml
      - action: add-label@v1
        args:
          label: iOS CI
